#Total mapping rules per class of HBsRE-KG

PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX improvit:<http://www.project-improvit.de/vocab/>
PREFIX rr: <http://www.w3.org/ns/r2rml#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX onto: <http://www.ontotext.com/>


select distinct ?c1 (count(distinct ?o1)+1 as ?triples)
where {
       ?s <http://semweb.mmlab.be/ns/rml#logicalSource> ?c.
       ?s <http://www.w3.org/ns/r2rml#subjectMap> ?o.
       ?o <http://www.w3.org/ns/r2rml#class> ?c1 .
            OPTIONAL {?s <http://www.w3.org/ns/r2rml#predicateObjectMap> ?o1 .
                     ?o1 <http://www.w3.org/ns/r2rml#predicate> ?o4}
      } GROUP BY ?c1
         ORDER BY DESC(?triples)
