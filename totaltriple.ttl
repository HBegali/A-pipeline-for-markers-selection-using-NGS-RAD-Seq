#Total number of triples in HBsRE-KG

PREFIX rml:<http://semweb.mmlab.be/ns/rml#>
PREFIX rr: <http://www.w3.org/ns/r2rml#>
SELECT  (COUNT(DISTINCT ?triplesmap) as ?numberMappings) WHERE # it counting the triple
{ ?triplesmap rml:logicalSource ?o .
?o ?typeDefinition ?source .
?triplesmap rr:subjectMap ?o2 .
?o2 rr:class ?class . }

#OR   #// Get the number of triples //
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX improvit:<http://www.project-improvit.de/vocab/>
PREFIX rr: <http://www.w3.org/ns/r2rml#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX onto: <http://www.ontotext.com/>
SELECT (COUNT(*) as ?Triples) WHERE { ?s ?p ?o }
